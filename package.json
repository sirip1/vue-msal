{
  "name": "@sirip1/vue-msal",
  "version": "3.2.0",
  "description": "Vue plugin for using Microsoft Authentication Library (MSAL)",
  "main": "lib/plugin.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:all",
    "build:all": "tsc -p ./tsconfig.json",
    "prepack": "npm run build"
  },
  "keywords": [
    "vue",
    "msal",
    "adal",
    "authentication",
    "sso"
  ],
  "author": "Marios Vertopoulos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sirip1/vue-msal/issues"
  },
  "contributors": [
    {
      "name": "Marios Vertopoulos"
    }
  ],
  "eslintIgnore": [
    "lib/*.*",
    "dist/*.*"
  ],
  "homepage": "https://github.com/sirip1/vue-msal#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirip1/vue-msal.git"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.21",
    "msal": "^1.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.138"
  }
}
